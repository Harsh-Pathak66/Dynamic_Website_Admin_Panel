    </head>
<body>
    
    <%-include('../../partials/admin/header.ejs')%>

    <section id="mid">
        <div class="container">
            <div class="row">
                <%-include('../../partials/admin/left.ejs')%>
                <div class="col-md-9">
                    <h2 class="text-center">Query Reply Form</h2>
                    <form action="" method="post">
                        <label for="">To</label>
                        <input type="text" name="emailto" id="" class="form-control" value="<%=record.email%>">
                        <label for="">From</label>
                        <input type="text" name="emailfrom" id="" class="form-control" value='fullstackproject12@gmail.com'>
                        <label for="">Subject</label>
                        <input type="text" name="subject" id=""  class="form-control"  value="Reply : <%=record.query%>">
                        <label for="">Body</label>
                        <textarea name="body" id="" class="form-control" ></textarea>
                        <button type="submit" class="btn btn-warning form-control mt-2">Send</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
 
   <%-include('../../partials/admin/footer.ejs')%>

</body>
</html>